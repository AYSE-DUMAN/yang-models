<?xml version='1.0' encoding='UTF-8'?>
<data xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
  <interfaces xmlns="http://cisco.com/ns/yang/Cisco-IOS-XR-um-interface-cfg">
    <interface>
      <interface-name/>
      <sub-interface-type>
        <l2transport/>
        <point-to-point/>
        <multipoint/>
      </sub-interface-type>
      <ipv4/>
      <ipv6/>
      <dampening>
        <decay-half-life>
          <value/>
          <reuse-threshold>
            <value/>
            <suppress-threshold>
              <value/>
              <max-suppress-time>
                <value/>
                <restart-penalty>
                  <value/>
                </restart-penalty>
              </max-suppress-time>
            </suppress-threshold>
          </reuse-threshold>
        </decay-half-life>
      </dampening>
      <encapsulation>
        <ppp/>
        <hdlc/>
        <mfr/>
        <frame-relay>
          <IETF/>
        </frame-relay>
      </encapsulation>
      <shutdown/>
      <mtu/>
      <logging>
        <events>
          <link-status/>
        </events>
      </logging>
      <bandwidth/>
      <description/>
    </interface>
    <interface-preconfigure>
      <interface-name/>
      <sub-interface-type>
        <l2transport/>
        <point-to-point/>
        <multipoint/>
      </sub-interface-type>
      <ipv4/>
      <ipv6/>
      <dampening>
        <decay-half-life>
          <value/>
          <reuse-threshold>
            <value/>
            <suppress-threshold>
              <value/>
              <max-suppress-time>
                <value/>
                <restart-penalty>
                  <value/>
                </restart-penalty>
              </max-suppress-time>
            </suppress-threshold>
          </reuse-threshold>
        </decay-half-life>
      </dampening>
      <encapsulation>
        <ppp/>
        <hdlc/>
        <mfr/>
        <frame-relay>
          <IETF/>
        </frame-relay>
      </encapsulation>
      <shutdown/>
      <mtu/>
      <logging>
        <events>
          <link-status/>
        </events>
      </logging>
      <bandwidth/>
      <description/>
    </interface-preconfigure>
  </interfaces>
</data>
